<section id="heroSec" class="relative lg:pt-20">
    <!-- Video for hero background -->
    <video id="heroVideo" class="absolute top-0 left-0 w-full h-full object-cover" autoplay muted loop playsinline
        preload="auto">
        <source id="videoSource" src="/images/javolin_vid.mp4" type="video/mp4">
        Your browser does not support the video tag.
    </video>
    <!-- Control icons -->
    <div id="controls" class="absolute bottom-4 right-4 flex space-x-4 z-10">
        <i id="playPauseIcon"
            class="fa-solid fa-pause text-white text-xl sm:text-2xl h-10 w-10 sm:h-12 sm:w-12 flex items-center justify-center rounded-full cursor-pointer hover:bg-opacity-70"></i>
        <i id="muteIcon"
            class="fa-solid fa-volume-mute text-white text-xl sm:text-2xl h-10 w-10 sm:h-12 sm:w-12 flex items-center justify-center bg-black bg-opacity-50 rounded-full cursor-pointer hover:bg-opacity-70"></i>
    </div>
</section>

<script>
    function updateVideoSource() {
        var videoSource = document.getElementById('videoSource');
        var screenWidth = window.innerWidth;

        if (screenWidth <= 768) {
            // Mobile and tablet devices
            videoSource.src = '/images/javolin_vid_mobile.mp4';
        } else {
            // Desktop devices
            videoSource.src = '/images/javolin_vid.mp4';
        }

        // Reload the video element to apply the new source
        var heroVideo = document.getElementById('heroVideo');
        heroVideo.load();
    }

    // Initial update on page load
    updateVideoSource();

    // Update video source on window resize
    window.addEventListener('resize', updateVideoSource);
</script>


