<section id="products"
    class="relative py-4 lg:px-24 bg-cover bg-center bg-no-repeat lg:bg-[url('/images/new-value-bg.png')] lg:bg-fixed"
    x-data="carousel()" x-init="init()" style="height: 800px; overflow: hidden;">
    <!-- Carousel Title -->
    <h2
        class="lg:slide lg:fadeRight lg:animation-element mt-14 mx-10 text-center text-justify text-slate-900 text-xl sm:text-xl md:text-xl lg:text-3xl font-medium  md:leading-normal lg:leading-relaxed font-['Satoshi']">
        At JAVOLIN we focus on combining the digital and physical worlds. We impact industries with our tech by
        accelerating
        efficiencies in payments, inventory management, logistics tracking and owned commodities.
    </h2>

    <!-- Carousel container -->
    <div class="valuepropositioncarousel flex justify-center items-center h-full lg:slide lg:fadeRight lg:animation-element"
        style="perspective: 1000px; position: relative; overflow: visible; width: 90%; margin: 0 auto; margin-top: -6rem;">

        <!-- Left navigation arrow with circle -->
        {{{leftArrowSVG}}}

        <!-- Right navigation arrow with circle -->
        {{{ rightArrowSVG }}}

        <!-- Carousel slides -->
        <template x-for="(slide, index) in slides" :key="index">
            <div @click="setActiveSlide(index)" @mouseenter="mouseEnter(index)" @mouseleave="mouseLeave(index)"
                class="absolute transition-transform duration-300 ease-in-out cursor-pointer"
                :class="{ 'active': slide.active, 'inactive': !slide.active }" :style="slideStyles(index).style">

                <!-- Card -->
                <div :class="slideStyles(index).class"
                    style="position: relative; padding: 1rem; border-radius: 1rem; margin-top: -6rem; perspective: 1000px;">
                    <div :class="{ 'flip-card-inner': true, 'flipped': slide.flipped }"
                        style="transition: transform 0.8s;">
                        <div class="flip-card-front"
                            style="position: absolute; width: 100%; height: 100%; backface-visibility: hidden;">
                            <img :src="slide.image" class="object-contain"
                                :style="'height: calc(2/3 * ' + slideHeight + 'px); width: 100%;'" />
                            <!-- Text Label -->
                            <div class="absolute bottom-15 left-0 w-full text-center"
                                style="padding: 1rem; bottom: 3rem;">
                                <span x-text="slide.label" class="text-xl font-bold font-['Satoshi']"></span>
                            </div>
                        </div>
                        <div class="flip-card-back bg-blue-800"
                            style="border-radius: 1rem; position: absolute; width: 100%; height: 100%; color: white; display: flex; align-items: flex-start; justify-content: center; backface-visibility: hidden; transform: rotateY(180deg); padding-top: 1rem;">
                            <div class="flex flex-col items-center text-center space-y-1" style="margin-top: 0.5rem;">
                                <!-- Adjust margin to move the title up -->
                                <span x-text="slide.label"
                                    class="w-56 text-center text-white text-2xl font-bold font-['Satoshi'] mb-10 my"></span>
                                <div style="width: 280px; text-align: center; color: white; font-size: 18px; font-family: Satoshi; font-weight: 400; "
                                    class="mt-10">
                                    <span x-text="slide.content"></span>
                                </div>
                            </div>
                        </div>
                    </div>

                    <!-- Arrow -->
                    <div class="arrow-container"
                        style="position: absolute; bottom: 15px; left: 50%; transform: translateX(-50%) rotate(-45deg); transform-origin: center;">
                        <!-- SVG for active card's arrow {{{activeCardArrowSVG}}} -->
                    </div>
                </div>
            </div>
        </template>
    </div>

    <!-- Navigation Dots -->
    <div class="absolute bottom-10 left-1/2 transform -translate-x-1/2 flex space-x-2">
        <template x-for="(slide, index) in slides" :key="index">
            <div @click="setActiveSlide(index)" class="cursor-pointer">
                <!-- Active dot with slight raise -->
                <div x-show="slide.active" class="transform -translate-y-0.5">
                    <svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <circle cx="8" cy="8" r="8" fill="#6CC24A" />
                    </svg>
                </div>
                <!-- Non-active dot -->
                <div x-show="!slide.active">
                    <svg width="12" height="12" viewBox="0 0 12 12" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <circle cx="6" cy="6" r="6" fill="#CAD9C5" />
                    </svg>
                </div>
            </div>
        </template>
    </div>
</section>



